    "number_of_samples = 300000\n",
    "lower_bound = np.array([0.05,0.25,2])\n",
    "upper_bound = np.array([2,1.3,15])\n",
    "res_RS = random_search(f,number_of_samples,\n",
    "                    lower_bound,upper_bound)"
   ]
